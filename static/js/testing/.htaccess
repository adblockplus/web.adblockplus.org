RewriteCond %{ENV:GEOIP_COUNTRY_CODE} !^(US|CA|AU|NZ)$ [OR]
RewriteCond %{ENV:GEOIP_COUNTRY_CODE_V6} !^(US|CA|AU|NZ)$
